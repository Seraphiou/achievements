{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never",
        "functions": "never"
      }
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ]
  }
}